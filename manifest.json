{
  "name": "BitKip extension Downloader ",
  "action": {},
  "manifest_version": 3,
  "version": "1.0",
  "description": "Download BitKip.",
  "host_permissions": ["*://*/*"],
  "permissions": [
    "downloads",
    "cookies",
    "webRequest",
    "tabs",
    "storage",
    "contextMenus",
    "alarms",
    "scripting"
  ],
  "background": {
    "service_worker": "connector.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["http://*/*", "https://*/*", "ftp://*/*", "file:///*"],
      "js": ["content.js"]
    }
  ],
  
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
